class Geeks {
    static void printInFormat(float a, float b) {
        float result = a / b;  //Store division of a/b in a new variable result
        
        // Print the result up to 3 decimal places
        System.out.print(result);
        System.out.format(" %.3f", result);
    }   
    public static void main(String[] args) {
        //call function and print result
        Geeks.printInFormat(5.43f, 2.653f);  
        Geeks.printInFormat(3.25f, 2.5f);    
    } 
}
